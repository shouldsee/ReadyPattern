<?xml version="1.0"?>
<VTKFile type="ImageData" version="0.1" byte_order="LittleEndian" compressor="vtkZLibDataCompressor">
  <RD format_version="4">
    <description>
      k and F are fixed to display this specific replicator behavior.
      
      Notice this replicator follow roughly a seesaw growth rate, as opposed to steady growth rate seen in previous GS replicator.
      
      Reported by shouldsee
      
    </description>
    <rule name="Gray-Scott" type="formula" wrap="0" neighborhood_type="vertex" neighborhood_range="1" neighborhood_weight="laplacian">
      <param name="timestep">
        0.2
      </param>
      <param name="diff">
        0.07
      </param>
      <param name="k1">
        0.03
      </param>
      <param name="k2">
        0.17
      </param>
      <param name="F1">
        0
      </param>
      <param name="F2">
        3
      </param>
      <param name="vis">
        1600
      </param>
      <param name="asp">
        20
      </param>
      <param name="dx">
        0.6
      </param>
      <param name="F0">
        0.03
      </param>
      <formula number_of_chemicals="3">
        //float4 hpos = index_x / (float)X; // [0,1]
        //float4 vpos = index_y / (float)Y; // [0,1]
        float4 hpos=0.85f;
        float4 vpos=0.95f;
        
        float4 k = k1 + (k2-k1)*hpos;
        float4 F = F1 + (F2-F1)*vpos;
        delta_a = (2.0f * diff * laplacian_a/dx/dx - F*a*b*b + F0*(1.0f-a))*dx;
        delta_b = (diff * laplacian_b/dx/dx + F*a*b*b - (F0+k)*b)*dx;
        c=delta_a*vis*dx;
        
      </formula>
    </rule>
    <initial_pattern_generator apply_when_loading="false">
      <overlay chemical="a">
        <overwrite>
        </overwrite>
        <constant value="1">
        </constant>
        <everywhere>
        </everywhere>
      </overlay>
      <overlay chemical="b">
        <overwrite>
        </overwrite>
        <white_noise low="0.5" high="1">
        </white_noise>
        <rectangle>
          <point3D x="0.2" y="0.2" z="0.6">
          </point3D>
          <point3D x="0.4" y="0.5" z="0.8">
          </point3D>
        </rectangle>
      </overlay>
    </initial_pattern_generator>
    <render_settings>
